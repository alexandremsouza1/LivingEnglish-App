<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.livingenglish.quasar" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>livingEnglish</name>
    <description>Projeto para aprender ingles rapido e divertido</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <universal-links>
        <host name="livingenglish.page.link" scheme="https" />
        <host name="living-english-274111.firebaseapp.com" scheme="https">
            <path url="/__/auth/callback" />
        </host>
    </universal-links>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AndroidLaunchMode" value="singleTask" />
    <preference name="android-minSdkVersion" value="19" />
    <platform name="android">
        <allow-intent href="market:*" />
        <splash density="land-ldpi" src="www/screen/android/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="www/screen/android/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="www/screen/android/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="www/screen/android/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="www/screen/android/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="www/screen/android/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="www/screen/android/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="www/screen/android/drawable-port-mdpi-screen.png" />
        <splash density="port-hdpi" src="www/screen/android/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="www/screen/android/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="www/screen/android/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="www/screen/android/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <allow-navigation href="about:*" />
</widget>
